{
  "name": "agent",
  "version": "1.0.0",
  "description": "AI Bouncer for Token Access Control",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --exec ts-node src/agents/knowledgeCovalent/knowledgeCovalentExample",
    "start": "cross-env NODE_ENV=production pm2 start dist/index.js --name backend --no-daemon",
    "prod": "npm run build && cross-env NODE_ENV=production pm2 start ecosystem.config.js --env production"
  },
  "dependencies": {
    "@covalenthq/ai-agent-sdk": "^0.2.4",
    "@langchain/core": "^0.1.17",
    "@langchain/openai": "^0.0.12",
    "dotenv": "^16.4.7",
    "langchain": "^0.1.12",
    "openai": "^4.28.0",
    "pm2": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "pino-pretty": "^13.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.0"
  }
}
