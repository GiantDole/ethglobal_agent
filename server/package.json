{
  "name": "backend",
  "version": "1.0.0",
  "description": "A Node.js backend built with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon --watch src --exec ts-node src/index.ts",
    "start": "cross-env NODE_ENV=production pm2 start dist/index.js --name backend --no-daemon",
    "prod": "npm run build && cross-env NODE_ENV=production pm2 start ecosystem.config.js --env production"
  },
  "dependencies": {
    "@covalenthq/ai-agent-sdk": "^0.2.4",
    "@langchain/core": "^0.3.39",
    "@langchain/google-genai": "^0.1.8",
    "@langchain/openai": "^0.4.3",
    "@privy-io/server-auth": "^1.18.4",
    "@supabase/supabase-js": "^2.48.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "express": "^4.21.2",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "langchain": "^0.3.15",
    "next": "^15.1.6",
    "openai": "^4.28.0",
    "pino": "^9.6.0",
    "pm2": "^5.2.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15",
    "pino-pretty": "^13.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.0"
  }
}
